    <rdf:Description rdf:about="http://www.i-knex.com/ontology/hadith#Believer">
        <terms:description xml:lang="en">A Believer (Muslim) who may or may not be a companion .</terms:description>
        <rdfs:seeAlso xml:lang="en">https://dbpedia.org/page/Muslims</rdfs:seeAlso>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.i-knex.com/ontology/hadith#Companion">
        <terms:description xml:lang="en">Companion to Prophet Muhammad (peace be upon him)</terms:description>
