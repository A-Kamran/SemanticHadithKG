                                  owl:onProperty :title ;
                                  owl:someValuesFrom xsd:string
                                ] .


###  http://www.i-knex.com/ontology/hadith#HadithText
:HadithText rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :isPartOfHadith ;
                              owl:someValuesFrom :Hadith
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hadithText ;
                              owl:someValuesFrom xsd:string
                            ] .


###  http://www.i-knex.com/ontology/hadith#HadithType
:HadithType rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :elevated
                                              :sacred
                                              :severed
                                              :stopped
                                            )
                                ] ;
            <http://purl.org/dc/terms/description> "Different terms are used for the origin of a narration. These terms specify whether a narration is attributed to Muhammad, a companion, a successor or a latter historical figure." .


###  http://www.i-knex.com/ontology/hadith#NarratorChain
:NarratorChain rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://schema.org/isPartOf> ;
                                 owl:someValuesFrom :Hadith
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasNarratorSegment ;
                                 owl:someValuesFrom :NarratorChainSegment
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasRootNarratorSegment ;
                                 owl:someValuesFrom :RootNarratorChainSegment
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isPartOfHadith ;
                                 owl:someValuesFrom :Hadith
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :narratorChain ;
                                 owl:someValuesFrom xsd:string
                               ] .


###  http://www.i-knex.com/ontology/hadith#NarratorChainSegment
:NarratorChainSegment rdf:type owl:Class ;
                      rdfs:subClassOf owl:Thing ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :refersToNarrator ;
                                        owl:allValuesFrom :HadithNarrator
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :refersToNarrator ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger
                                      ] .


###  http://www.i-knex.com/ontology/hadith#RootNarrator
:RootNarrator rdf:type owl:Class ;
              rdfs:subClassOf :HadithNarrator .


###  http://www.i-knex.com/ontology/hadith#RootNarratorChainSegment
:RootNarratorChainSegment rdf:type owl:Class ;
                          rdfs:subClassOf :NarratorChainSegment ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :heardFrom ;
                                            owl:maxCardinality "0"^^xsd:nonNegativeInteger
                                          ] ;
                          rdfs:comment "The Root Narrator (First Narrator Occurrence) has no heardFrom since they are the eyewitness to the Hadith contents" .


###  http://www.i-knex.com/ontology/hadith#Sacred_Hadith
:Sacred_Hadith rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Hadith
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasHadithType ;
                                                            owl:hasValue :sacred
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] .


###  http://www.i-knex.com/ontology/hadith#Severed_Hadith
:Severed_Hadith rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Hadith
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasHadithType ;
                                                             owl:hasValue :severed
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Hadith .


###  http://www.i-knex.com/ontology/hadith#Stopped_Hadith
:Stopped_Hadith rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Hadith
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasHadithType ;
                                                             owl:hasValue :stopped
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Hadith .


###  http://www.i-knex.com/ontology/hadith#Verse
:Verse rdf:type owl:Class .


